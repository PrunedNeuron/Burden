{
	"name": "burden",
	"displayName": "Unpacked Dependency Burden",
	"author": {
		"name": "ayush",
		"email": "am@ayushm.dev",
		"url": "https://ayushm.dev"
	},
	"description": "Display the total unpacked size of the dependencies in current nodejs project.",
	"license": "MIT",
	"version": "0.0.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/PrunedNeuron/burden.git"
	},
	"icon": "",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/package.json"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.48.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-no-secrets": "^0.6.8",
		"eslint-plugin-no-unsanitized": "^3.1.2",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-simple-import-sort": "^5.0.3",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"prettier": "^2.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"axios": "^0.20.0",
		"pretty-bytes": "^5.3.0"
	}
}
